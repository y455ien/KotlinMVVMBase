<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.kotlinmvvmbase.ui.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_homeInternal"
            app:destination="@id/homeInternal" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.kotlinmvvmbase.ui.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.kotlinmvvmbase.ui.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/homeInternal"
        android:name="com.example.kotlinmvvmbase.ui.HomeInternal"
        android:label="fragment_home_internal"
        tools:layout="@layout/fragment_home_internal" />
</navigation>